{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "id": {
      "type": "string"
    },
    "order": {
      "type": "number",
      "minimum": 0
    },
    "initialBPM": {
      "type": "number",
      "minimum": 0
    },
    "program": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "rgb": {
            "type": "array",
            "items": {
              "type": "number",
              "minimum": 0,
              "maximum": 255
            },
            "minItems": 3,
            "maxItems": 3
          },
          "type": {
            "type": "number",
            "minimum": 0
          },
          "dotted": {
            "type": "boolean"
          }
        },
        "required": ["rgb"]
      }
    }
  },
  "required": ["name", "id", "order", "initialBPM", "program"]
}
